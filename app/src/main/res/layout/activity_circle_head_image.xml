<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mogul.xxm.devtools.CircleHeadImageActivity">

    <com.mogul.xxm.libdevtools.circleview.CircleImageView
        android:id="@+id/normal_circle_view"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true"
        app:civ_border_color="@color/black_de"
        app:civ_border_overlay="true"
        app:civ_fill_color="@color/colorPrimary"
        app:civ_border_width="2dp"
        android:src="@mipmap/test02"/>

    <com.mogul.xxm.libdevtools.circleview.CircleImageView
        android:id="@+id/net_circle_view"
        android:layout_below="@+id/normal_circle_view"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:civ_border_color="@color/black_de"
        app:civ_border_overlay="true"
        app:civ_fill_color="@color/colorPrimary"
        app:civ_border_width="2dp"
        android:src="@mipmap/test01"/>

</RelativeLayout>
